import sys

print 50
min=0
max=100
intento=50
while True:
    resp=raw_input()
    print "resp: "+resp
    if (resp.find("menor")):
        max=intento
        intento=int((max-min)/2)
    elif (resp.find("mayor")):
        min=intento
        intento=int((max-min)/2)
    elif (resp.find("Intento n")):
        print "estoy aqu√≠"
        print intento
    elif (resp.find("Lo siento")):
        pass
    elif (resp.find("Quires jugar")):
        print "n"
        break
    else:
        sys.stderr.write('Error Desconocido. Fin.\n')
        sys.exit(1)
      
