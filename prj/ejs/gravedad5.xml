<Type>LIBRARY_EDITOR</Type>
<Name>Página Propia</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
int numPuntosEscala(int numPuntos, double distancia, int valorMax) {
   int res=(int) ((double)numPuntos/valorMax*Math.pow(distancia,0.333333)*2.2);
   //int res=(int) (Math.pow(distancia,0.333333)*2.2);
   if (res<1) {
      res=1;
   } else if (res>valorMax) {
      res=valorMax;
   }
   return res;
}
public void cambiaPuntosEstela() {
   numPuntosEstelaMercurio=numPuntosEscala(numPuntosEstela,cuasiDistanciaMercurio,1000);
   numPuntosEstelaVenus=numPuntosEscala(numPuntosEstela,cuasiDistanciaVenus,1000);
   numPuntosEstelaTierra=numPuntosEscala(numPuntosEstela,cuasiDistanciaTierra,1000);;
}
]]></Code>

</Content>
