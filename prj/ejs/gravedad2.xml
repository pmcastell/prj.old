<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Evolución Mercurio</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
dx=mx-solx;
dy=my-soly;
dz=mz-solz;
gm_raiz=-GM/Math.pow(dx*dx+dy*dy+dz*dz,1.5);
ax[0]=gm_raiz*(mx-solx); // aceleración gravitatoria
ay[0]=gm_raiz*(my-soly);
az[0]=gm_raiz*(mz-solz);
vx[0]+=ax[0]*incT;
vy[0]+=ay[0]*incT;
vz[0]+=az[0]*incT;
mx+=vx[0]*incT;
my+=vy[0]*incT;
mz+=vz[0]*incT;

]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Evolución Venus</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
dx=vex-solx;
dy=vey-soly;
dz=vez-solz;
gm_raiz=-GM/Math.pow(dx*dx+dy*dy+dz*dz,1.5);
ax[1]=gm_raiz*(vex-solx); // aceleración gravitatoria
ay[1]=gm_raiz*(vey-soly);
az[1]=gm_raiz*(vez-solz);
vx[1]+=ax[1]*incT;
vy[1]+=ay[1]*incT;
vz[1]+=az[1]*incT;
vex+=vx[1]*incT;
vey+=vy[1]*incT;
vez+=vz[1]*incT;
]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Evolución Tierra</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
dx=tx-solx;
dy=ty-soly;
dz=tz-solz;
gm_raiz=-GM/Math.pow(dx*dx+dy*dy+dz*dz,1.5);
ax[2]=gm_raiz*(tx-solx); // aceleración gravitatoria
ay[2]=gm_raiz*(ty-soly);
az[2]=gm_raiz*(tz-solz);
vx[2]+=ax[2]*incT;
vy[2]+=ay[2]*incT;
vz[2]+=az[2]*incT;
tx+=vx[2]*incT;
ty+=vy[2]*incT;
tz+=vz[2]*incT;
]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
<Osejs.Model.Constraints.Page>
