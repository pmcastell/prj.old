<Osejs.Model.Evolution>
<Osejs.Model.Evolution.Page>
<Type>CODE_EDITOR</Type>
<Name>Evolución Mercurio</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
for (int i=1; i<=avance; i++) {
   int precision=20;
   for(int j=1;j<=precision; j++) {
      dx=mx-solx;
      dy=my-soly;
      dz=mz-solz;
      cuasiDistanciaMercurio=Math.pow(dx*dx+dy*dy+dz*dz,1.5);
      gm_raiz=-GM/cuasiDistanciaMercurio;
      ax[0]=gm_raiz*(mx-solx); // aceleración gravitatoria
      ay[0]=gm_raiz*(my-soly);
      az[0]=gm_raiz*(mz-solz);
      mvx+=ax[0]*incT/precision;
      mvy+=ay[0]*incT/precision;
      mvz+=az[0]*incT/precision;
      mx+=mvx*incT/precision;
      my+=mvy*incT/precision;
      mz+=mvz*incT/precision;
   }
}
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
<Osejs.Model.Evolution.Page>
<Type>CODE_EDITOR</Type>
<Name>Evolución Venus</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
for (int i=1; i<=avance; i++) {
   double precision=20;
   for(int j=1;j<=precision; j++) {
      dx=vex-solx;
      dy=vey-soly;
      dz=vez-solz;
      cuasiDistanciaVenus=Math.pow(dx*dx+dy*dy+dz*dz,1.5);
      gm_raiz=-GM/cuasiDistanciaVenus;
      ax[1]=gm_raiz*(vex-solx); // aceleración gravitatoria
      ay[1]=gm_raiz*(vey-soly);
      az[1]=gm_raiz*(vez-solz);
      vevx+=ax[1]*incT/precision;
      vevy+=ay[1]*incT/precision;
      vevz+=az[1]*incT/precision;
      vex+=vevx*incT/precision;
      vey+=vevy*incT/precision;
      vez+=vevz*incT/precision;
   }
}
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
<Osejs.Model.Evolution.Page>
<Type>CODE_EDITOR</Type>
<Name>Evolución Tierra</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
for (int i=1; i<=avance; i++) {
   double precision=20.0D;
   for(int j=1;j<=precision; j++) {
      dx=tx-solx;
      dy=ty-soly;
      dz=tz-solz;
      cuasiDistanciaTierra=Math.pow(dx*dx+dy*dy+dz*dz,1.5);
      gm_raiz=-GM/cuasiDistanciaTierra;
      ax[2]=gm_raiz*(tx-solx); // aceleración gravitatoria
      ay[2]=gm_raiz*(ty-soly);
      az[2]=gm_raiz*(tz-solz);
      tvx+=ax[2]*incT/precision;
      tvy+=ay[2]*incT/precision;
      tvz+=az[2]*incT/precision;
      tx+=tvx*incT/precision;
      ty+=tvy*incT/precision;
      tz+=tvz*incT/precision;
   }
}
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
