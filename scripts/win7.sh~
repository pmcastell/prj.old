#!/bin/bash
#if [ "$(whoami)" != "root" ]; then gksudo $0: exit ; fi
USUPASS=$(zenity --password --title="Instroduce contraseÃ±a de usuario")
if [ ! -f /l/miL/virtualbox-enc/win7/win7.vbox ]; then
   PASS=""
   PASS=$(echo $PASS | desencripta)
   echo $USUPASS | sudo bash -c "echo $PASS | encfs -S --public /l/miL/.virtualbox.enc /l/miL/virtualbox-enc"
fi   
sudo -H -u usuario vboxmanage startvm win7 --type headless &
ping -c 4 win7 &> /dev/null
sudo iptables -F
ping -c 4 win7 &> /dev/null
sudo firewall
sleep 20
sudo -H -u usuario vncviewer win7 &

